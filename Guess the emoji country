javascript:(function() {
    const countries = [
        { name: "France", emojis: "ðŸ¥–ðŸ·" },
        { name: "Japan", emojis: "ðŸ£ðŸŽŽ" },
        { name: "Brazil", emojis: "ðŸŒ´âš½" },
        { name: "Australia", emojis: "ðŸ¨ðŸ„â€â™‚ï¸" },
        { name: "Canada", emojis: "ðŸðŸ’" },
        { name: "Germany", emojis: "ðŸºðŸ‚" },
        { name: "Italy", emojis: "ðŸ•ðŸ" },
        { name: "England", emojis: "â˜•ðŸŽ¡" },
        { name: "usa", emojis: "ðŸ”ðŸŽ¬" },
        { name: "India", emojis: "ðŸ•ŒðŸ•‰ï¸" },
        { name: "Iceland", emojis: "ðŸ¥¶ðŸ›¬" }
    ];

    const randomCountry = countries[Math.floor(Math.random() * countries.length)];
    let attempts = 3;
    let guessedCorrectly = false;

    while (attempts > 0 && !guessedCorrectly) {
        const guess = prompt(`Guess the country from these emojis: ${randomCountry.emojis}\nYou have ${attempts} attempts left:`).trim();

        if (guess.toLowerCase() === randomCountry.name.toLowerCase()) {
            alert("Congratulations! You guessed the country correctly.");
            guessedCorrectly = true;
        } else {
            attempts -= 1;
            if (attempts > 0) {
                alert("Incorrect guess. Try again.");
            } else {
                alert(`Sorry, you're out of attempts. The correct answer was ${randomCountry.name}.`);
            }
        }
    }
})();
